


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Backend5</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/WebApplication1.styles.css" asp-append-version="true" />

    <style>
    header {background-color: black; padding:5px;}
    .headlinks {color: grey; text-decoration: none;} 
    </style>
</head>

<body>
    <header>
        <nav @*class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3"*@>
            <div class="container-fluid">
                <a span style = "font-size: 20px; color: grey; text-decoration: none; margin-left:80px; margin-right:30px;"  asp-area="" asp-controller="Home" asp-action="Index"><b>Backend5</b></a>
                <a class="headlinks" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                <a class="headlinks" span style = "margin-left: 25px;"  asp-area="" asp-controller="Hospitals" asp-action="Index">Hospitals</a>
                <a class="headlinks" span style = "margin-left: 25px;"  asp-area="" asp-controller="Labs" asp-action="Index">Labs</a>
                <a class="headlinks" span style = "margin-left: 25px;"  asp-area="" asp-controller="Doctors" asp-action="Index">Doctors</a>
                <a class="headlinks" span style = "margin-left: 25px;"  asp-area="" asp-controller="Patients" asp-action="Index">Patients</a>
              @*  <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">BackEnd1</a>
                <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a> *@
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                           @* <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>*@
                        </li>
                      @*  <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li> *@
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - Backend5 @*- <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>*@
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
